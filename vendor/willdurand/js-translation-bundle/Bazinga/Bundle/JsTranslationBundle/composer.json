{
    "name": "willdurand/js-translation-bundle",
    "description": "A pretty nice way to expose your translation messages to your JavaScript.",
    "keywords": [ "translation", "javascript", "symfony" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
        "name": "William Durand",
        "email": "william.durand1@gmail.com"
    }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/console": "~2.3",
        "symfony/intl": "~2.3"
    },
    "require-dev": {
        "symfony/yaml": "~2.3",
        "symfony/browser-kit": "~2.3",
        "symfony/twig-bundle": "~2.3"
    },
    "replace": {
        "willdurand/expose-translation-bundle": "2.2.*"
    },
    "autoload": {
        "psr-0": { "Bazinga\\Bundle\\JsTranslationBundle": "" }
    },
    "target-dir": "Bazinga/Bundle/JsTranslationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
